<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%- title %></title>
</head>

<body>
    <%- include("partials/navbar") %>
    <% if(locals.user) { %>
        <p>Hello, <%= user.username %>
        </p>
    <% } %>
    <% if (locals.messages) { %>
        <ul>
        <% messages.forEach((message) => { %>
            <li><%= message %></li>
        <% }) %>
        </ul>
    <% } %>
</body>

</html>